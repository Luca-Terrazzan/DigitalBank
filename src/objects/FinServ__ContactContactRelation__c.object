    <validationRules>
        <fullName>FinServ__CCRCanOnlyBeDeactivated</fullName>
        <active>true</active>
        <errorConditionFormula>AND(OR(ISCHANGED(FinServ__Contact__c),ISCHANGED(FinServ__RelatedContact__c),ISCHANGED(FinServ__Role__c)),NOT(ISNEW()))</errorConditionFormula>
        <errorMessage>This record can&apos;t be edited. To update relationship information, you can deactivate this record and create a new one.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FinServ__CannotCreateInactiveRecord</fullName>
        <active>true</active>
        <description>Ensures that a record cannot be created as Inactive</description>
        <errorConditionFormula>AND(ISNEW(),FinServ__Active__c == false)</errorConditionFormula>
        <errorMessage>A Contact-Contact Relationship cannot be created as Inactive.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FinServ__ContactAndRelatedContactCannotBeSame</fullName>
        <active>true</active>
        <errorConditionFormula>FinServ__Contact__c = FinServ__RelatedContact__c</errorConditionFormula>
        <errorMessage>Contact and Related Contact must be different entities.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FinServ__InactiveCCRCannotBeEdited</fullName>
        <active>true</active>
        <errorConditionFormula>AND(PRIORVALUE(FinServ__Active__c) == false,NOT(ISNEW()))</errorConditionFormula>
        <errorMessage>Inactive record can&apos;t be edited. To update relationship information, you can create a new one.</errorMessage>
    </validationRules>
