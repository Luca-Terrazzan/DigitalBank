    <validationRules>
        <fullName>Closed_Referral_read_only</fullName>
        <active>true</active>
        <description>When a referral is changed to &quot;Create Opportunity&quot; or &quot;No Opportunity&quot; status, the Referral Name and Account fields need to be made read only.</description>
        <errorConditionFormula>IF( or( ISPICKVAL( Referral_Status__c ,&apos;Create Opportunity&apos;), ISPICKVAL( Referral_Status__c ,&apos;No Opportunity&apos;)), or( ISCHANGED( Account__c ), ISCHANGED( Name )),false)</errorConditionFormula>
        <errorMessage>If the Referral Status is Create Opportunity or No Opportunity, the Referral and Account Name fields cannot be changed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reason_Lost_if_Status_equals_No_Oppty</fullName>
        <active>true</active>
        <description>Populate Reason Lost field on referral if the Referral Status is updated to &quot;No Opportunity&quot;</description>
        <errorConditionFormula>AND(
 ISPICKVAL( Referral_Status__c , &quot;No Opportunity&quot;),
 ISPICKVAL( Reason_Lost__c , &quot;&quot;) 
 )</errorConditionFormula>
        <errorDisplayField>Reason_Lost__c</errorDisplayField>
        <errorMessage>The Reason Lost field needs to be populated if the Referral Status is &quot;No Opportunity&quot;</errorMessage>
    </validationRules>
